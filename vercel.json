{
  "functions": {
    "server/entry.express.js": {
      "memory": 3000,
      "maxDuration": 900
    }
  },
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    }
  ]
}
